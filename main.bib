@article {Yasuda2019,
	author = {Yasuda, Hiromi and Miyazawa, Yasuhiro and Charalampidis, Efstathios G. and Chong, Christopher and Kevrekidis, Panayotis G. and Yang, Jinkyu},
	title = {Origami-based impact mitigation via rarefaction solitary wave creation},
	volume = {5},
	number = {5},
	pages= {eaau2835},
	year = {2019},
	publisher = {American Association for the Advancement of Science},
	abstract = {The principles underlying the art of origami paper folding can be applied to design sophisticated metamaterials with unique mechanical properties. By exploiting the flat crease patterns that determine the dynamic folding and unfolding motion of origami, we are able to design an origami-based metamaterial that can form rarefaction solitary waves. Our analytical, numerical, and experimental results demonstrate that this rarefaction solitary wave overtakes initial compressive strain waves, thereby causing the latter part of the origami structure to feel tension first instead of compression under impact. This counterintuitive dynamic mechanism can be used to create a highly efficient{\textemdash}yet reusable{\textemdash}impact mitigating system without relying on material damping, plasticity, or fracture.},
	URL = {https://advances.sciencemag.org/content/5/5/eaau2835},
	journal = {Sci. Adv.}
}
