journal = {Phys. Rev. Lett.},
month = {jul},
number = {2},
pages = {024301},
title = {{Demonstrating an In Situ Topological Band Transition in Cylindrical Granular Chains}},
url = {https://link.aps.org/doi/10.1103/PhysRevLett.119.024301},
